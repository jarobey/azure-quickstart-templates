{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.1",
  "parameters": {
    "resourceAPIVersion":{
      "type":"string"
    },
    "networkSpec": {
      "type": "object"
    },
    "masterASName": {
      "type": "string"
    },
    "dataASName": {
      "type": "string"
    },
    "utilityASName": {
      "type": "string"
    }
  },
  "variables": {},
  "resources": [
    {
      "apiVersion": "[parameters('resourceAPIVersion')]",
      "type": "Microsoft.Compute/availabilitySets",
      "name": "[parameters('masterASName')]",
      "location": "[resourceGroup().location]",
      "properties": { }
    },
    {
      "apiVersion": "[parameters('resourceAPIVersion')]",
      "type": "Microsoft.Compute/availabilitySets",
      "name": "[parameters('dataASName')]",
      "location": "[resourceGroup().location]",
      "properties": { }
    },
    {
      "apiVersion": "[parameters('resourceAPIVersion')]",
      "type": "Microsoft.Compute/availabilitySets",
      "name": "[parameters('utilityASName')]",
      "location": "[resourceGroup().location]",
      "properties": { }
    }
  ]
}
